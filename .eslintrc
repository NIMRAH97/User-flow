{
  "env": {
    "node": true
  },
  "extends": ["airbnb-base", "plugin:node/recommended", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": "warn",
    "no-underscore-dangle": "off",
    "consistent-return": "warn",
    "no-unused-vars": "warn",
    "radix": "off"
  }
}
